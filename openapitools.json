{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "6.0.1",
    "generators": {
      "import-bug": {
        "generatorName": "typescript-angular",
        "output": "output-6.0.1/#{name}",
        "inputSpec": "import-bug.json"
      },
      "export-bug": {
        "generatorName": "typescript-angular",
        "output": "output-6.0.1/#{name}",
        "inputSpec": "export-bug.json",
        "additionalProperties": {
          "useSingleRequestParameter": "true"
        }
      },
      "join-bug": {
        "generatorName": "typescript-angular",
        "output": "output-6.0.1/#{name}",
        "inputSpec": "join-bug.json"
      }
    }
  }
}
